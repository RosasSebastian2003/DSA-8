program fibonacci_recursivo;
var
    n : int;
    resultado : int;

int fibonacci(x : int) [
    {
        if (x < 2) {
            return x;
        } else {
            return fibonacci(x - 1) + fibonacci(x - 2);
        };
    }
];

main {
    n = 10;
    resultado = fibonacci(n);
    print("El fibonacci de", n, "es:", resultado);
}
end